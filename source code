#include <iostream>
#include<math.h>
using namespace std;

int main() {
	int n;
    cin>>n;
    int sum=0;
    int orignaln=n;
while(n>0)
{

int l=n%10;
sum+= pow(l,3);
n=n/10;
cout<<n<<" "<<sum;
}
cout<<endl;
if(sum==orignaln)
{
    cout<<"yes"<<sum;

}
else{
    cout<<"no"<<sum;
}



	return 0;
}

